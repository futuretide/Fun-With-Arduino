int out1 = 9
int out2 = 8;
int out3 = 7;
int out4 = 6;
void setup()
{
  pinMode(9,OUTPUT);
  pinMode(8,OUTPUT);
  pinMode(7,OUTPUT);
  pinMode(6,OUTPUT);
}

void loop()
{
    analogWrite(out1,analogRead(A0));
    analogWrite(out2,analogRead(A1));
    analogWrite(out3,analogRead(A3));
    analogWrite(out4,analogRead(A4)); 
}
